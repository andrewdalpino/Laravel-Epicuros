{
    "name": "andrewdalpino/epicuros",
    "description": "An Epicuros service provider for Laravel and Lumen.",
    "license": "MIT",
    "keywords": ["laravel", "lumen", "auth", "jwt", "authentication", "context", "microservices"],
    "authors": [
        {
            "name": "Andrew DalPino",
            "email": "me@andrewdalpino.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/http": ">=5.2",
        "illuminate/support": ">=5.2",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "AndrewDalpino\\LaravelEpicuros\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
